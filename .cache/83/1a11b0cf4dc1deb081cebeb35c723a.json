 display: flex;\r\n        align-items: flex-start;\r\n        .panel-effect {\r\n            width: 100vw;\r\n            height: 100%;\r\n            display: flex;\r\n            flex-direction: column;\r\n            justify-content: center;\r\n            .cover {\r\n                width: 100vw;\r\n                height: 76vw;\r\n                display: flex; \r\n                justify-content: center;\r\n                .square {\r\n                    width: 76vw;\r\n                    height: 76vw;\r\n                    border-radius: 5px;\r\n                    overflow: hidden;\r\n                    box-shadow: 0px 20px 40px 0px rgba(212,212,212,0.7);\r\n                    img {\r\n                        width: 100%;\r\n                        height: 100%;\r\n                    }\r\n                }\r\n                \r\n            }\r\n            .mini-lyric {\r\n                height: 4em;\r\n                padding-top: 1.8em;\r\n                p {\r\n                    text-align: center;\r\n                    font-size: 14px;\r\n                    color: $grey;\r\n                    margin: 0.5em 0;\r\n                    &.current {\r\n                        color: $black;\r\n                    }\r\n                }\r\n            }\r\n            \r\n        }\r\n        .panel-lyrics {\r\n            width: 100vw;\r\n            height: 100%;\r\n            .container {\r\n                transition:  all 0.3s;\r\n                p {\r\n                    text-align: center;\r\n                    font-size: 14px;\r\n                    color: $grey;\r\n                    margin: 1em 0;\r\n                    &.current {\r\n                        color: $black;\r\n                        font-weight: bold;\r\n                    } \r\n                }\r\n            }\r\n        }\r\n    }\r\n    .songMain.active {\r\n        transform: translateX(-100vw);\r\n    }\r\n    .songBottom {\r\n        .option {\r\n           