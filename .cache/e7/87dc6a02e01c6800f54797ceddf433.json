per.js","original":{"line":36,"column":5}},{"generated":{"line":56,"column":3},"source":"src/js/swiper.js","original":{"line":31,"column":4}},{"generated":{"line":57,"column":0},"source":"src/js/swiper.js","original":{"line":37,"column":3}},{"name":"Swiper","generated":{"line":59,"column":15},"source":"src/js/swiper.js","original":{"line":40,"column":15}}],"sources":{"src/js/swiper.js":"class Swiper {\n  constructor(node) {\n    if(!node) throw new Error('需要传递需要绑定的DOM元素')\n    let root = typeof node === 'string' ? document.querySelector(node) : node\n    let eventHub = {'swipLeft': [], 'swipRight': []}\n    let initX\n    let newX\n    let clock\n    root.ontouchstart = function(e) {\n      initX = e.changedTouches[0].pageX \n    }\n\n    root.ontouchmove  = function(e) {\n      if(clock) clearInterval(clock)\n      clock = setTimeout(()=>{\n        newX = e.changedTouches[0].pageX\n        if(newX - initX > 50) {\n          eventHub['swipRight'].forEach(fn=>fn.bind(root)())\n        } else if(initX - newX > 50) {\n          eventHub['swipLeft'].forEach(fn=>fn.bind(root)())\n        }  \n      }, 100)\n    }\n\n    this.on = function (type, fn) {\n      if(eventHub[type]) {\n        eventHub[type].push(fn)\n      }\n    }\n\n    this.off = function (type, fn) {\n      let index = eventHub[type].indexOf(fn)\n      if( index !== -1) {\n        eventHub[type].splice(index, 1)\n      }\n    }\n  }\n}\n\nexport default Swiper\n\n"},"lineCount":null}},"error":null,"hash":"604f246095e8d5e98743d1e13fa084af","cacheData":{"env":{}}}